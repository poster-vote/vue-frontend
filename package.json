{
  "name": "vue-frontend",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --write */**/*.{js,vue,json,css,md}",
    "gen-readme-toc": "md-toc -i"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.1",
    "bulma": "^0.7.2",
    "color": "^3.1.0",
    "dayjs": "^1.8.0",
    "vue": "^2.5.22",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@robb_j/md-toc": "^0.2.2",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "casex": "^3.0.0",
    "eslint": "^5.12.1",
    "eslint-plugin-vue": "^5.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "node-sass": "^4.13.1",
    "prettier": "^1.16.1",
    "pug": "^3.0.1",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.0.1",
    "sharp": "^0.21.3",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.5.22"
  }
}
